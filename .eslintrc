{
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "rules": {
    "import/default": 0,
    "import/no-duplicates": 0,
    "import/named": 0,
    "import/namespace": 0,
    "import/no-unresolved": 0,
    "import/no-named-as-default": 0,
    "comma-dangle": 0,
    "indent": [2, 2, {"SwitchCase": 1}],
    "no-console": 0,
    "no-alert": 0,
    "max-len":[2, 140],
    "func-names":0
  },
  "plugins": [
    "import"
  ],
  "parser": "babel-eslint"
}
